{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:make": "node-pg-migrate create -m ./migrations",
    "db:migrate": "dotenv -e .env -- node-pg-migrate up -m ./migrations -d $DATABASE_URL",
    "db:rollback": "dotenv -e .env -- node-pg-migrate down -m ./migrations -d $DATABASE_URL"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.2",
    "@types/pg": "^8.15.5",
    "dotenv-cli": "^10.0.0",
    "node-pg-migrate": "^8.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
